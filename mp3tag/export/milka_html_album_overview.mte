var curr_theme = new Array();	//current theme
var old_theme = 0;	//nr of link in info table to current theme
var old_link = 0;	//nr of link in info table to old theme

function UpdateLinks(theme_nr, link_nr) {	//updates colors of links to themes, direction and type of effect in the info table
	//direction
	var obj = document.getElementById("dir_" + Number(1-gdirection));	//clear color of old link
	if (obj) { 
		obj.style.color = '''';
		if (!document.styleSheets) { obj.style.color = curr_theme'['5']' }
	}
	var obj = document.getElementById("dir_" + gdirection);	//set color of new link
	if (obj) { obj.style.color = curr_theme'['6']' }
	//color themes
	if (Number(theme_nr)>=0) {
		var obj = document.getElementById("ct_" + old_theme);
		if (obj) { 
			obj.style.color = '''';
			if (!document.styleSheets) { obj.style.color = curr_theme'['5']' }
		}
		old_theme = Number(theme_nr);
		var obj = document.getElementById("ct_" + theme_nr);
		if (obj) { obj.style.color = curr_theme'['6']' }
	}
	//type
	if (Number(link_nr)>=0) {
		var obj = document.getElementById("type_" + old_link);
		if (obj) { 
			obj.style.color = '''';
			if (!document.styleSheets) { obj.style.color = curr_theme'['5']' }
		}
		old_link = Number(link_nr);
		var obj = document.getElementById("type_" + link_nr);
		if (obj) { obj.style.color = curr_theme'['6']' }
	}
}

function ChooseRules(rules_array) {	//makes an array with numbers of css rules which will be used in SetColorTheme()
	var numbers = new Array(18);
	for (i=0;i<18;i++) { numbers'['i']' = 0; }	//prevents exceptions if some class will not be found -> body background color will be changed instead
	var s = new String("");
	for (i=0;i<rules_array.length;i++) { 
		s = rules_array'['i']'.selectorText.toLowerCase();	//IE css names are uppercase
		switch (s) {
			case ''body'': { numbers'['0']' = i; break; }				//0
			case ''.main_title'': { numbers'['1']' = i; break; }		//1

			case ''.info_table'': { numbers'['2']' = i; break; }		//2
			case ''.info_names'': { numbers'['3']' = i; break; }		//3
			case ''.info_values'': { numbers'['4']' = i; break; }		//4
			case ''.info_table a'': { numbers'['5']' = i; break; }	//5
			case ''.info_table a:hover'': { numbers'['6']' = i; break; }	//6
			case ''.info_line'': { numbers'['7']' = i; break; }		//7

			case ''.export_header'': { numbers'['8']' = i; break; }	//10 11
			case ''.export_table'': { numbers'['9']' = i; break; }	//12 15
			case ''.export_table a'': { numbers'['10']' = i; break; }	//13
			case ''.export_table a:hover'': { numbers'['11']' = i; break; }//14

			case ''.about_table'': { numbers'['12']' = i; break; }	//16 17
			case ''.about_table a'': { numbers'['13']' = i; break; }	//18
			case ''.about_table a:hover'': { numbers'['14']' = i; break; }//19

			case ''.dhtmltooltip'': { numbers'['15']' = i; break; }	//16 17
			case ''.export_header a'': { numbers'['16']' = i; break; }	//11
			case ''.export_header a:hover'': { numbers'['17']' = i; break; }	//11
		}
	}
	return numbers;
}

function SetColorTheme(name, update_now, theme_nr) {	//applies a color theme to document by it''s name and updates the table if necessary; name = name of the array with colors (s.th. like "ct_red")
	curr_theme = eval(name);
	if (!curr_theme) { return };
	var all_rules = new Array();
	var all_nums = new Array();
	
	SetGradientColor(curr_theme'['8']', curr_theme'['9']');	//set up export table backgrounds 1 & 2
	if (document.styleSheets) {	//mozilla/ie browsers allows change css rules
		if (document.styleSheets'['0']'.rules) { all_rules =  document.styleSheets'['0']'.rules;}	//IE
		if (document.styleSheets'['0']'.cssRules) { all_rules =  document.styleSheets'['0']'.cssRules;}	//Netscape
	
		if (!all_rules) {return};	//unknown browser that supports document.styleSheets but neither rules nor cssRules
		all_nums = ChooseRules(all_rules);
	
		all_rules'['all_nums'['0']'']'.style.backgroundColor = curr_theme'['0']';	//background
		all_rules'['all_nums'['1']'']'.style.color = curr_theme'['1']';	//main title

		all_rules'['all_nums'['2']'']'.style.backgroundColor  = curr_theme'['2']';	//info table background
		all_rules'['all_nums'['3']'']'.style.color = curr_theme'['3']';	//info table names
		all_rules'['all_nums'['4']'']'.style.color = curr_theme'['4']';	//info table values
		all_rules'['all_nums'['5']'']'.style.color = curr_theme'['5']';	//info table link
